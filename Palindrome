

class Solution
{
    public String is_palindrome(int n)
    { 
        String s = String.valueOf(n);
        char[] arr =s.toCharArray();
        int j = arr.length-1;
        
        for( int i =0; i<=j; i++){
            if ( arr[i] != arr[j]){
             return "No";
            }
            j--;
        }
       return "Yes"; 
    }
}
