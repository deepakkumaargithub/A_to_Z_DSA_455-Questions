Input: 5

Output:
* * * * *
* * * * *
* * * * *
* * * * *
* * * * *



class Solution {

    void printSquare(int n) {
        for (int row = 1; row<=n; row++){
            for ( int col =1; col<=n; col++ ){
                System.out.print("* ");
            }
            System.out.println();
        }
    }
}
